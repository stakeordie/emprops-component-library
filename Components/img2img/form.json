{
  "workflow": "img2img",
  "name": "Image to Image",
  "params": {
    "main": [
      {
        "name": "Select Image",
        "id": "image",
        "type": "image_loader"
      },
      {
        "name": "Model",
        "id": "model",
        "conf_file": "sd_models.json"
      },
      {
        "name": "Denoising Strength",
        "id": "denoising",
        "type": "number",
        "min": 0,
        "max": 1,
        "step": 0.01
      },
      {
        "name": "Prompt",
        "id": "prompt",
        "type": "textarea"
      },
      {
        "name": "Negative Prompt",
        "id": "negative_prompt",
        "type": "textarea"
      },
      {
        "name": "Aspect Ratio",
        "id": "aspect_ratio",
        "conf_file": "aspect_ratio.yaml"
      },
      {
        "name": "Steps",
        "id": "steps",
        "type": "number",
        "min": 1,
        "max": 60,
        "step": 20
      }
    ],
    "advanced": [
      {
        "name": "Sampler",
        "id": "sampler",
        "default": "Euler a",
        "conf_file": "samplers.yaml"
      },
      {
        "name": "Scheduler",
        "id": "scheduler",
        "default": "Karras",
        "conf_file": "samplers.yaml"
      },
      {
        "name": "Guidance (CFG)",
        "id": "cfg",
        "type": "number",
        "default": 7,
        "min": 0,
        "max": 50,
        "steps": 0.01
      }
    ]
  }
}