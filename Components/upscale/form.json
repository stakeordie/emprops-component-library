{
  "main": [
    {
      "id": "image",
      "name": "Select Image",
      "type": "image_loader",
      "default": "",
      "display": true
    },
    {
      "name": "Model",
      "id": "model",
      "type": "select",
      "default": "4x-UltraSharp.pth",
      "conf_file": "upscale-models.json",
      "display": true
    },
    {
      "name": "Scale By",
      "id": "scale_by",
      "type": "slider",
      "default": 2,
      "constraints": {
        "min": 2,
        "max": 4,
        "step": 1
      },
      "display": true
    }
  ],
  "advanced": [
    {
      "name": "Upscale Method",
      "id": "upscale_method",
      "type": "select",
      "default": "nearest-exact",
      "conf_file": "upscale-method.json",
      "display": true
    }
  ]
}
