{
  "name": "Text to Image",
  "fields": {
    "main": [
      {
        "id": "model",
        "name": "Model",
        "type": "select",
        "conf_file": "sd_models.json",
        "display": true
      },
      {
        "id": "prompt",
        "name": "Prompt",
        "type": "prompt_editor",
        "type": "textarea",
        "display": true
      },
      {
        "id": "negative_prompt",
        "name": "Negative Prompt",
        "type": "prompt_editor",
        "type": "textarea",
        "display": true
      },
      {
        "id": "aspect_ratio",
        "name": "Aspect Ratio",
        "type": "aspect_ratio",
        "conf_file": "aspect_ratio.json",
        "display": true
      },
      {
        "id": "steps",
        "name": "Steps",
        "type": "slider",
        "default": 20,
        "constraints": {
          "min": 1,
          "max": 60,
          "step": 20
        },
        "display": true
      }
    ],
    "advanced": [
      {
        "id": "sampler",
        "name": "Sampler",
        "default": "euler_ancestral",
        "conf_file": "samplers.json",
        "display": true
      },
      {
        "id": "scheduler",
        "name": "Scheduler",
        "default": "karras",
        "conf_file": "schedulers.json",
        "display": true
      },
      {
        "id": "cfg",
        "name": "Guidance (CFG)",
        "type": "slider",
        "default": 7,
        "constraints": {
          "min": 0,
          "max": 50,
          "steps": 0.01
        },
        "display": true
      }
    ]
  }
}
