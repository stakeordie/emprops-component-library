# AWS Configuration
AWS_DEFAULT_REGION="your-aws-region"
AWS_ACCESS_KEY_ID="your-aws-access-key"
AWS_SECRET_ACCESS_KEY="your-aws-secret-key"

# API Keys
OPENAI_API_KEY="your-openai-api-key"
HF_TOKEN="your-huggingface-token"

# LangFlow Configuration
LANGFLOW_AUTO_LOGIN="false"
LANGFLOW_NEW_USER_IS_ACTIVE="false"
LANGFLOW_SUPERUSER="your-username"
LANGFLOW_SUPERUSER_PASSWORD="your-password"
LANGFLOW_SECRET_KEY="your-secret-key"
LANGFLOW_VARIABLES_TO_GET_FROM_ENVIRONMENT="OPENAI_API_KEY"

# Server Credentials
SERVER_CREDS="your-server-credentials"

